{
    "name": "sprite",
    "displayName": "squigglier",
    "description": "svg animator",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "squigglier",
            "aliases": ["squigglier", "squigglier"],
            "extensions": [".sprite"],
            "configuration": "./language-configuration.json",
            "embeddedLanguages": {
                "text.html.basic": "html",
                "source.js": "javascript"
            }
        }],
        "grammars": [{
            "language": "squigglier",
            "scopeName": "source.sprite",
            "path": "./syntaxes/squigglier.tmLanguage.json",
            "embeddedLanguages": {
                "text.html.basic": "html",
                "source.js": "javascript"
            }
        }]
    }
}